{
  "name": "server-hono",
  "tasks": {
    "build": "deno task update_symlinks ; deno task build:vite",
    "update_symlinks": "./update_symlinks.sh",
    "build:vite": "deno run -A npm:vite build",
    "start": "deno run --allow-net --allow-ffi --allow-read --allow-env --allow-sys --allow-write --allow-run --node-modules-dir ./src/main.ts",
    "cf:dev": "deno run -A --no-check npm:wrangler dev",
    "cf:deploy": "deno run -A --no-check npm:wrangler deploy"
  },
  "license": "MIT",
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "vite": "npm:vite@7.1.4",
    "vite-plugin-wasm": "npm:vite-plugin-wasm@3.5.0",
    "@vitejs/plugin-vue": "npm:@vitejs/plugin-vue@6.0.1",
    "hono": "npm:hono@4.10.7",
    "@hono/vite-ssg": "npm:@hono/vite-ssg@0.3.0",
    "ventojs": "npm:ventojs@2.3.0"
  },
  "exports": {}
}
